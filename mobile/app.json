{
  "expo": {
    "name": "Apartment Bill Tracker",
    "slug": "apartment-bill-tracker",
    "icon": "./src/assets/icon.png",
    "version": "1.1.4",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "aptbilltracker",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apartmentbilltracker.ios",
      "scheme": "aptbilltracker"
    },
    "android": {
      "package": "com.apartmentbilltracker",
      "scheme": "aptbilltracker"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icon.png",
          "colors": ["#b38604"],
          "modes": ["production"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "72a4be98-f7c2-4e5c-8d5a-5b7a8fd1a75c"
      }
    },
    "owner": "apartmentbilltracker"
  }
}
