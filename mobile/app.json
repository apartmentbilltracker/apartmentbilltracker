{
  "expo": {
    "name": "Apartment Bill Tracker",
    "slug": "apartment-bill-tracker",
    "icon": "./src/assets/icon.png",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "aptbilltracker",
    "cli": {
      "appVersionSource": "dynamic"
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.apartmentbilltracker.ios",
      "scheme": "aptbilltracker"
    },
    "android": {
      "bundleIdentifier": "com.apartmentbilltracker.android",
      "package": "com.apartmentbilltracker",
      "scheme": "aptbilltracker"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icon.png",
          "colors": ["#b38604"],
          "modes": ["production"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2c14cabc-44e4-4116-8a15-99bfe4feee8a"
      }
    },
    "owner": "mjbelia"
  }
}
